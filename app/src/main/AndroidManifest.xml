<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="robertcurtis.jingnicai.sidiamadou.tutor.tutorapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TutorActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LandingPageActivity" />
        <activity
            android:name=".SkillPageActivity"
            android:label="@string/title_activity_skill_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TutorSearchActivity"
            android:label="@string/title_activity_tutor_search"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".StudentSearchActivity"
            android:label="@string/title_activity_student_search"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MyStudentsActivity"
            android:label="@string/title_activity_my_students"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MyTutorsActivity"
            android:label="@string/title_activity_my_tutors"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>